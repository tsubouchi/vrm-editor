{
  "version": 2,
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["all"],
  "outputDirectory": ".next",
  "crons": [],
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_GEMINI_API_KEY": "${NEXT_PUBLIC_GEMINI_API_KEY}"
  }
} 